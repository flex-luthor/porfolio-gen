    "material-ui-upload": "^1.2.1",
    "moment": "2.24.0",
    "node-sass": "^4.13.1",
    "nouislider": "14.0.2",
    "prop-types": "15.7.2",
    "react": "16.9.0",
    "react-anchor-link-smooth-scroll": "^1.0.12",
    "react-datetime": "2.16.3",
    "react-dom": "16.9.0",
    "react-firebase-file-uploader": "^2.4.3",
    "react-hot-loader": "^4.12.19",
    "react-router-dom": "5.0.1",
    "react-s3": "^1.3.1",
    "react-scripts": "^3.3.1",
    "react-scroll": "^1.7.16",
