{
  "name": "material-kit-react",
  "version": "1.8.0",
  "description": "Material Kit React. Coded by Creative Tim",
  "private": true,
  "main": "dist/index.js",
  "dependencies": {
    "@firebase/storage": "^0.3.26",
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
